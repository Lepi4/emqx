{
    "name": "EMQX-proxy",
    "version": "0.2.0",
    "slug": "EMQX__proxy",
    "description": "Proxy for externally running Zigbee2MQTT",
    "url": "https://github.com/zigbee2mqtt/hassio-zigbee2mqtt/tree/master/zigbee2mqtt-proxy",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "set-none",
    "map": [
      "config:ro"
    ],
    "ports": {
      "2020/tcp": null
    },
    "ports_description": {
      "2020/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    },
    "image": "zigbee2mqtt/zigbee2mqtt-proxy-{arch}"
}
