{
    "name": "EMQX Proxy",
    "version": "1",
    "slug": "EMQX_proxy",
    "description": "Proxy for externally running EMQX",
    "url": "https://github.com/Lepi4/emqx/tree/master/zigbee2mqtt-proxy",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:emqx",
    "map": [
      "config:ro"
    ],
    "ports": {
      "18083/tcp": null
    },
    "ports_description": {
      "18083/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    },
    "image": "zigbee2mqtt/zigbee2mqtt-proxy-{arch}"
}
